
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  nome      String
  sobrenome String
  cpf       String   @unique
  senha     String
  telefone  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}